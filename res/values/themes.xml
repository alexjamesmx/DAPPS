<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.T201" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/cyan</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name='Theme.201.Botones_calculadora' parent='Theme.T201'>
        <item name='android:layout_weight'>1</item>
        <item name='android:layout_height'>match_parent</item>
        <item name='backgroundTint'>@color/fondo_boton_cal</item>
        <item name='android:layout_marginStart'>4dp</item>
        <item name='android:layout_marginEnd'>4dp</item>
        <item name='android:textSize'>16sp</item>
    </style>
    <style name='Theme.201.Practica_2_cuadros' parent='Theme.T201'>
        <item name='android:layout_width'>1dp</item>
        <item name='android:layout_height'>match_parent</item>
        <item name='android:layout_weight'>6</item>
        <item name='android:gravity'>center</item>
        <item name='android:orientation'>vertical</item>
        <item name='android:weightSum'>1</item>
    </style>
    <style name='Theme.201.Practica_2_imagenes' parent='Theme.T201'>
        <item name='android:layout_width'>80dp</item>
        <item name='android:layout_height'>1dp</item>
        <item name='android:fillColor'>@color/practica_2_imagenes</item>
        <item name='android:tint'>@color/practica_2_imagenes</item>
        <item name='android:layout_weight'>0.5</item>
        <item name='android:alpha'>0.6</item>
    </style>
    <style name='Theme.201.Practica_2_text' parent='Theme.T201'>
        <item name='android:textSize'>12sp</item>
        <item name='android:layout_width'>wrap_content</item>
        <item name='android:layout_height'>wrap_content</item>
        <item name='android:textColor'>@color/white</item>
    </style>


    <!--SWITCH -->
</resources>